{
  "name": "wikibase-solutions/php-cypher-dsl",
  "description": "A PHP DSL for the Cypher Query Language",
  "type": "library",
  "homepage": "https://wikibase-solutions.com",
  "keywords": [
    "neo4j",
    "cypher",
    "dsl",
    "graph"
  ],
  "license": "GPL-v2.0-or-later",
  "authors": [
    {
      "name": "Marijn van Wezel",
      "role": "Developer"
    },
    {
      "name": "Lars Akkermans",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-ctype": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0"
  },
  "autoload": {
    "psr-4": {
      "WikibaseSolutions\\CypherDSL\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WikibaseSolutions\\CypherDSL\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit tests/"
  },
  "minimum-stability": "dev"
}